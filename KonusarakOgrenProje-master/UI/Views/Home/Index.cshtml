@model List<EntityLayer.Concrete.Product>
@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-md-3">

        @await Component.InvokeAsync("Categories")
        @await Component.InvokeAsync("Marka")

    </div>


    <div class="col-md-9">
        <div class="row">
            @foreach (var product in Model)
            {
                <div class="col-md-4">
                   @await Html.PartialAsync("_Product", product)
                </div>
            }
        </div>
    </div>
</div>
